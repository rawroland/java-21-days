
public class StringChecker {

  public static void main(String[] args) {
    String str = "Nobody ever went broke buying IBM";
    System.out.println("The string is: " + str);
    System.out.println("Length of this string: " + str.length());
    System.out.println("The character at position 5: " + str.charAt(5));
    System.out.println("The string from 26 to 32: " + str.substring(26, 32));
    System.out.println("The index of the character: " + str.indexOf('v'));
  }
}
